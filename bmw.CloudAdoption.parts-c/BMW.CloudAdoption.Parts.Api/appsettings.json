{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "KafkaTopics" :{
    "Parts": ""
  },
  "Kafka": {
    "BootstrapServers": "",
    "SaslUsername": "",
    "SaslPassword": "",
    "groupId": "",
    
    "SaslMechanism": "PLAIN",
    "SecurityProtocol": "SaslPlaintext",
    "SocketKeepaliveEnable": "True",
    "SslEndpointIdentificationAlgorithm": "Https",
    "Acks": "all",
    
    "AutoOffsetReset": "earliest",
    "EnableAutoCommit": "false",
    "AutoCommitIntervalMs": 5000
  }
}
